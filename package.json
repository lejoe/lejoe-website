    "@nuxtjs/google-fonts": "^3.2.0",
    "@vite-pwa/nuxt": "^0.10.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.5.1",
    "postcss-html": "^1.7.0",
    "prettier": "^3.4.2",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^24.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
